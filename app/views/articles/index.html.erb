<h1>This is a list of articles</h1>
<p>
<%= link_to "Create a new article", new_article_path %>
</p>
<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
  </tr>

<%  @articles.each do |article| %>
<tr>
  <th><%= article.title %></th>
  <th><%= article.description %></th>
  <th><%= link_to "Edit", edit_article_path(article) %> </td>
  <th><%= link_to "Show", article_path(article) %> </td>

</tr>
<% end %>


</table>
